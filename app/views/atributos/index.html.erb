<p id="notice"><%= notice %></p>

<h1>Atributos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atributos.each do |atributo| %>
      <tr>
        <td><%= atributo.nombre %></td>
        <td><%= link_to 'Show', atributo %></td>
        <td><%= link_to 'Edit', edit_atributo_path(atributo) %></td>
        <td><%= link_to 'Destroy', atributo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atributo', new_atributo_path %>
