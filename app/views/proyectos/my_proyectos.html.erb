<%= stylesheet_link_tag "static_pages" %>

<div class="container">
  <p id="notice"><%= notice %></p>

  <h1> My Proyectos </h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Nombre</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @proyectos.each do |proyecto| %>
        <tr>
          <td><%= proyecto.nombre %></td>
          <td><%= link_to 'Mostrar', proyecto %></td>
          <td><%= link_to 'Editar', edit_proyecto_path(proyecto) %></td>
          <td><%= link_to 'Borrar Proyect', proyecto, method: :delete, data: { confirm: 'Estas Seguro?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>


</div>
